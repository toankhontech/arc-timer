{
  "name": "@ArcTimer/themes",
  "version": "0.0.0",
  "description": "Pre-built themes and theming utilities for ArcTimer",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist", "README.md"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@ArcTimer/core": "workspace:*"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "keywords": ["timer", "themes", "dark-mode", "theming"],
  "repository": {
    "type": "git",
    "url": "https://github.com/toankhontech/arc-timer.git",
    "directory": "packages/themes"
  },
  "homepage": "https://github.com/toankhontech/arc-timer",
  "bugs": {
    "url": "https://github.com/toankhontech/arc-timer/issues"
  },
  "author": "toankhontech",
  "license": "MIT"
}
